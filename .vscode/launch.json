{
    "configurations": [
        
        {
            "type": "node",
            "name": "vscode-jest-tests",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/.bin/jest",
            "args": [
                "test",
                "lib-name",
                "--runInBand",
                "true"
            ],
            "cwd": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}